<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.mythesis.ssh.model">

	<class name="Today" table="t_today">
		<id name="id">
            <generator class="native"/>
		</id>
		<property name="present" />
		<property name="six"/>
		<property name="nine" />
		<property name="twelve"/>
		<property name="fifteen" />
		<property name="eighteen"/>
		<property name="twentyOne" />
		<property name="twentyFour"/>
		<property name="count"/>
	</class>
	
</hibernate-mapping>
